        <Button BackgroundColor="#A3A09A" TextColor="White" FontSize="25" AbsoluteLayout.LayoutBounds="0.5, 0.2, 0.8, 0.1" AbsoluteLayout.LayoutFlags="All" CornerRadius="15" Text="History" Clicked="HistoryPage_Clicked">
        </Button>
        <Button BackgroundColor="#A3A09A" TextColor="White" FontSize="25" AbsoluteLayout.LayoutBounds="0.5, 3.5, 0.8, 0.1" AbsoluteLayout.LayoutFlags="All" CornerRadius="15" Text="Insight">
        </Button>
    </AbsoluteLayout>
</ContentPage>